{
  "name": "Weather-surf",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "dotenv -e .env -- ts-node-dev --transpile-only src/index.ts | pino-pretty -c",
    "test": "yarn lint && yarn test:unit && yarn test:functional --runInBand",
    "test:unit": "dotenv -e .env -- jest",
    "test:functional": "dotenv -e .env -- jest --projects ./tests --runInBand",
    "lint": "eslint ./src ./tests --ext .ts",
    "lint:fix": "eslint ./src ./tests --ext .ts --fix",
    "style:check": "prettier --check src/**/*.ts tests/**/*.ts",
    "style:fix": "prettier --write src/**/*.ts tests/**/*.ts"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/config": "^0.0.38",
    "@types/cors": "^2.8.12",
    "@types/express": "^4.17.11",
    "@types/express-pino-logger": "^4.0.2",
    "@types/express-rate-limit": "^5.1.3",
    "@types/http-status-codes": "^1.2.0",
    "@types/jest": "^26.0.22",
    "@types/jsonwebtoken": "^8.5.4",
    "@types/lodash": "^4.14.171",
    "@types/module-alias": "^2.0.0",
    "@types/mongoose": "^5.11.97",
    "@types/multer": "^1.4.7",
    "@types/nock": "^11.1.0",
    "@types/node": "^14.14.37",
    "@types/node-cache": "^4.2.5",
    "@types/pino": "^6.3.9",
    "@types/supertest": "^2.0.10",
    "@types/swagger-ui-express": "^4.1.3",
    "@typescript-eslint/eslint-plugin": "^2.29.0",
    "@typescript-eslint/parser": "^2.29.0",
    "dotenv-cli": "^3.2.0",
    "eslint": "^7.21.0",
    "jest": "^26.6.3",
    "nock": "^13.1.1",
    "prettier": "^2.2.1",
    "supertest": "^6.1.3",
    "ts-jest": "^26.5.4",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.2.3"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "axios": "^0.21.1",
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.0",
    "config": "^3.3.6",
    "cors": "^2.8.5",
    "express": "^4.17.1",
    "express-openapi-validator": "^4.12.15",
    "express-pino-logger": "^6.0.0",
    "express-rate-limit": "^5.3.0",
    "http-status-code": "^2.1.0",
    "http-status-codes": "^2.1.4",
    "jsonwebtoken": "^8.5.1",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "mongo": "^0.1.0",
    "mongoose": "^5.13.2",
    "node-cache": "^5.1.2",
    "pino": "^6.12.0",
    "pino-pretty": "^5.1.2",
    "swagger-ui-express": "^4.1.6"
  }
}
